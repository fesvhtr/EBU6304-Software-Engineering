<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AchievementInfoController">
    <children>
        <TableView fx:id="table" layoutX="41.0" layoutY="110.0" prefHeight="518.0" prefWidth="818.0">
            <columns>
                <TableColumn fx:id="sourceTypeCol" prefWidth="75.0" text="Module/Activity" />
                <TableColumn fx:id="sourceCol" prefWidth="75.0" text="Source" />
                <TableColumn fx:id="descriptionCol" prefWidth="91.5" text="Description" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <HBox layoutX="465.0" layoutY="55.0" prefHeight="31.0" prefWidth="341.0" spacing="20.0">
            <children>
                <JFXButton buttonType="RAISED" onAction="#newHandled" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="New" textFill="WHITE" />
                <JFXButton buttonType="RAISED" onAction="#editHandled" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="Edit" textFill="WHITE" />
                <JFXButton buttonType="RAISED" onAction="#delHandled" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="Delete" textFill="WHITE" />
            </children>
        </HBox>
        <Label layoutX="44.0" layoutY="56.0" prefHeight="28.0" prefWidth="341.0" text="Achievement Information list">
            <font>
                <Font name="Calibri" size="24.0" />
            </font>
        </Label>
    </children>
</AnchorPane>

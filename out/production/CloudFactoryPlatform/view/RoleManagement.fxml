<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RoleInfoController">
   <children>
      <TableView fx:id="table" layoutX="41.0" layoutY="109.0" prefHeight="518.0" prefWidth="818.0">
         <columns>
            <TableColumn fx:id="titleCol" prefWidth="200.0" text="Title" />
            <TableColumn fx:id="startCol" prefWidth="75.0" text="Start Date" />
            <TableColumn fx:id="endCol" prefWidth="75.0" text="End Date" />
            <TableColumn fx:id="desCol" prefWidth="75.0" text="Description" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox layoutX="469.0" layoutY="62.0" prefHeight="31.0" prefWidth="374.0" spacing="20.0">
         <children>
            <JFXButton buttonType="RAISED" layoutX="10.0" layoutY="10.0" maxHeight="31.0" maxWidth="110.0" onAction="#newHandled" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="New" textFill="WHITE" />
            <JFXButton buttonType="RAISED" maxHeight="31.0" maxWidth="120.0" onAction="#delHandled" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="Delete" textFill="WHITE" />
            <JFXButton buttonType="RAISED" maxHeight="31.0" maxWidth="100.0" onAction="#editHandled" prefHeight="0.0" prefWidth="100.0" style="-fx-background-color: #6E7F91;" text="Edit" textFill="WHITE" />
         </children>
      </HBox>
      <Label layoutX="58.0" layoutY="55.0" text="Roles Undertaken">
         <font>
            <Font name="Calibri" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
